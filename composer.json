{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "ext-bcmath": "*",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/dbal": "^3",
        "doctrine/doctrine-bundle": "^2",
        "doctrine/doctrine-migrations-bundle": "^3",
        "doctrine/orm": "^3",
        "symfony/console": "^7",
        "symfony/doctrine-messenger": "^7",
        "symfony/flex": "^2",
        "symfony/framework-bundle": "^6",
        "symfony/messenger": "^7",
        "symfony/runtime": "^7",
        "symfony/uid": "^7",
        "symfony/yaml": "^7"
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
    },
    "scripts": {
        "auto-scripts": {
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^7",
        "symfony/dotenv": "^7",
        "symfony/phpunit-bridge": "^7"
    }
}
